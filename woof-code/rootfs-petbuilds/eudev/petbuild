download() {
    [ -f eudev-3.2.9.tar.gz ] || wget -t 3 -T 60 https://dev.gentoo.org/~blueness/eudev/eudev-3.2.9.tar.gz
}

build() {
    tar -xzf eudev-3.2.9.tar.gz
    cd eudev-3.2.9
    ./configure --prefix=/usr --sysconfdir=/etc --libdir=/lib --bindir=/bin --sbindir=/sbin --disable-shared
    make install
}